.header {
    margin:20rpx 0 20rpx 60rpx;
    border-bottom: 1px solid #f8f8f8;
    text-align: center;
    width: 650rpx;
    height: 300rpx;
    line-height: 450rpx;
}

.title{
    margin-top: 35px;
    margin-left: 18px;
    color: #7a7878;
    font-size: 38rpx;
}

.icon{
    width: 300rpx;
    height: 300rpx;
}

.searchForm{
    border: rgb(72, 210, 252)
}

.searchForm input {
    margin: 10px;
}

.searchForm button {
    font-size: 38rpx;
    width: 40%;
    margin-top: 10rpx;
    height: 90rpx;
    margin-bottom: 10rpx;
}

.card {
    border: 2px solid #ffffff;
    border-radius: 5px;
    background-color: #ffffff;
    box-shadow: 4px 2px 1px #b9b7b7;
    margin: 15px;
    position: relative;
}

.info{
    font-size: 33rpx;
    color: #7a7878
}

.image {
    width: 100%;
    height: 200px;
    background-color: #eeeeee;
}

/*按钮*/
.loginBtnView {
    width: 100%;
    height: auto;
    background-color: #ffffff;
    margin-top: 0px;
    margin-bottom: 0px;
    padding-bottom: 0px;
  }